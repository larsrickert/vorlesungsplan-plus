{
  "name": "rickstack-template-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"./{src,tests}/**/*.{js,json,ts,html,scss,vue}\"",
    "format:hook": "pretty-quick --staged --pattern \"*(src|tests)/**/*.*(js|json|ts|html|scss|vue)\"",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/i18n/locales/**/*.json\"",
    "generate:resources": "npx cordova-res ios --skip-config --copy && npx cordova-res android --skip-config --copy",
    "update:package": "npx npm-check-updates -i"
  },
  "dependencies": {
    "@capacitor/browser": "^1.0.3",
    "@capacitor/core": "^3.2.3",
    "@capacitor/device": "^1.0.3",
    "@capacitor/network": "^1.0.3",
    "@capacitor/storage": "^1.2.0",
    "@ionic/vue": "^5.8.1",
    "@ionic/vue-router": "^5.8.1",
    "axios": "^0.21.1",
    "core-js": "^3.18.1",
    "pinia": "^2.0.0-rc.9",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.19",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.2.3",
    "@intlify/eslint-plugin-vue-i18n": "^0.15.0",
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-pwa": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "sass": "^1.42.1",
    "sass-loader": "^10.2.0",
    "typescript": "^4.4.3",
    "vue-cli-plugin-i18n": "~2.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:hook && npm run lint"
    }
  }
}
