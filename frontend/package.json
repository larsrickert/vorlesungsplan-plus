{
  "name": "vorlesungsplan-plus",
  "private": true,
  "version": "2.4.0",
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path ../.gitignore --fix src",
    "format": "prettier --write \"./{src,tests}/**/*.{js,json,ts,html,scss,vue}\" !src/assets",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@capacitor/browser": "^4.1.1",
    "@capacitor/core": "^4.8.1",
    "@capacitor/device": "^4.1.0",
    "@capacitor/local-notifications": "^4.1.5",
    "@capacitor/network": "^4.1.0",
    "@capacitor/preferences": "^4.0.2",
    "@ionic/vue": "^7.2.1",
    "@ionic/vue-router": "^7.2.1",
    "axios": "^1.4.0",
    "ionicons": "^7.1.2",
    "lottie-web": "^5.12.2",
    "pinia": "^2.1.6",
    "sanitize-html": "^2.11.0",
    "v-calendar": "^3.0.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.8.1",
    "@intlify/unplugin-vue-i18n": "^0.10.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.17.1",
    "@types/sanitize-html": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-vue": "^9.16.0",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "semantic-release": "^20.1.3",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.4.7",
    "vite-plugin-eslint": "~1.8.1",
    "vite-plugin-pwa": "^0.14.7",
    "vue-tsc": "^1.8.8",
    "workbox-window": "^6.6.0"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
